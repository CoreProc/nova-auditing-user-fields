{
    "name": "coreproc/nova-auditing-user-fields",
    "description": "A Laravel Nova field that works with the Laravel Auditing package to see who created or last updated a resource.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "owen-it/laravel-auditing": "^8.0 || ^9.0 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Coreproc\\NovaAuditingUserFields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coreproc\\NovaAuditingUserFields\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
